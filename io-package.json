{
    "common": {
        "name": "sql-community",
        "title": "SQL History Community",
        "titleLang": {
            "en": "SQL logging",
            "de": "SQL-Protokollierung",
            "ru": "Ведение журнала SQL",
            "pt": "Log de SQL",
            "nl": "SQL logging",
            "fr": "Journalisation SQL",
            "it": "Registrazione SQL",
            "es": "Registro de SQL",
            "pl": "Rejestrowanie SQL"
        },
        "desc": {
            "en": "Logging of states into SQL DB",
            "de": "Loggt die Historie von einzelnen Zuständen in einer SQL DB",
            "ru": "Сохраняет историю событий для отдельных состояний в SQL DB",
            "pt": "Registro de estados em SQL DB",
            "nl": "Logging van toestanden in SQL DB",
            "fr": "Journalisation des états dans la base de données SQL",
            "it": "Registrazione degli stati nel DB SQL",
            "es": "Registro de estados en SQL DB",
            "pl": "Rejestrowanie stanów w SQL DB"
        },
        "version": "0.0.1",
        "news": {
            "0.0.1": {
                "en": "Azure Sql Database support"
            }
        },
        "mode": "daemon",
        "platform": "Javascript/Node.js",
        "loglevel": "info",
        "messagebox": true,
        "subscribe": "messagebox",
        "keywords": [
            "charts",
            "sql",
            "logging",
            "graphs",
            "archive"
        ],
        "preserveSettings": "custom",
        "supportCustoms": true,
        "getHistory": true,
        "enabled": true,
        "stopBeforeUpdate": true,
        "materialize": true,
        "compact": true,
        "authors": [
            "foster",
        ],
        "license": "MIT",
        "readme": "https://github.com/ioBroker/ioBroker.sql/blob/master/README.md",
        "icon": "sql.png",
        "extIcon": "https://raw.githubusercontent.com/ioBroker/ioBroker.sql/master/admin/sql.png",
        "type": "storage",
        "dependencies": [
            {
                "js-controller": ">=2.0.0"
            }
        ],
        "globalDependencies": [
            {
                "admin": ">=1.6.10"
            }
        ],
        "supportStopInstance": 2000,
        "config": {
            "minWidth": 570,
            "width ": 435,
            "minHeight": 200,
            "height": 540
        },
        "plugins": {
            "sentry": {
                "dsn": "https://c30462d3e0834993b0aa29824346701e@sentry.iobroker.net/45"
            }
        }
    },
    "native": {
        "connLink": "",
        "debounce": 1000,
        "retention": 31536000,
        "host": "localhost",
        "port": 0,
        "user": "",
        "password": "",
        "dbtype": "sqlite",
        "fileName": "sqlite.db",
        "requestInterval": 0,
        "encrypt": false,
        "round": 4,
        "dbname": "iobroker",
        "multiRequests": true,
        "maxConnections": 100,
        "changesRelogInterval": 0,
        "changesMinDelta": 0,
        "writeNulls": true
    },
    "instanceObjects": [
        {
            "_id": "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id": "info.connection",
            "type": "state",
            "common": {
                "role": "indicator.connected",
                "name": "If connected to DB",
                "type": "boolean",
                "read": true,
                "write": false,
                "def": false
            },
            "native": {}
        }
    ],
    "objects": [
        {
            "_id": "_design/custom",
            "language": "javascript",
            "views": {
                "state": {
                    "map": "function(doc) { if (doc.type==='state' && (doc.common.custom || doc.common.history)) emit(doc._id, doc.common.custom || doc.common.history) }"
                }
            }
        }
    ]
}
